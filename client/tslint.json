{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "member-ordering": false,
    "prettier": [true, "./.prettierrc"]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}